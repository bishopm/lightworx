{
    "name": "bishopm/lightworx",
    "description": "Simple dev website",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bishop",
            "email": "michael@lightworx.co.za",
            "homepage": "https://lightworx.co.za"
        }
    ],
    "homepage": "https://github.com/bishopm/lightworx",
    "keywords": ["Laravel", "Filament"],
    "require": {
        "illuminate/support": "~12",
        "alareqi/filament-pwa": "~1",
        "filament/filament": "~4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Bishopm\\Lightworx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bishopm\\Lightworx\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bishopm\\Lightworx\\Providers\\LightworxServiceProvider"
            ],
            "aliases": {
                "Lightworx": "Bishopm\\Lightworx\\Facades\\Lightworx"
            }
        }
    }
}
